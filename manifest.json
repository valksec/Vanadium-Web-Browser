{
  "manifest_version": 3,
  "name": "Vanadium",
  "version": "1.0",
  "description": "extension",
  "permissions": ["activeTab", "<all_urls>"],
  "icons": {
    "48": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  },
  "action" {
     "default_popup": "popup.js",
     "default_icon": {
      "48": "icon.png"
    }
  }
}
